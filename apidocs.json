{
  "definition": {
    "openapi": "3.0.0",
    "info": {
      "title": "RestoPACK V1",
      "version": "1.0.0"
    },
    "tags": [
      {
        "name": "Auth",
        "description": "Authentication API"
      },
      {
        "name": "Users",
        "description": "User API"
      },
      {
        "name": "Tables",
        "description": "Table API"
      }
    ],
    "paths": {
      "/showalltable": {
        "get": {
          "tags": ["Tables"],
          "description": "Get all tables",
          "responses": {
            "200": {
              "description": "OK"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            }
          }
        }
      },
      "/createtable": {
        "post": {
          "tags": ["Tables"],
          "description": "Create a table",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "number": 1
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "OK"
            },
            "201": {
              "description": "Created"
            },
            "400": {
              "description": "Bad Request"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            }
          }
        }
      },
      "/deletetable/{id}": {
        "delete": {
          "tags": ["Tables"],
          "description": "Delete a table",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "Table id",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int64"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            },
            "404": {
              "description": "Not Found"
            }
          }
        }
      },
      "/login": {
        "post": {
          "tags": ["Auth"],
          "description": "Login",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "email": "admin@email.com",
                    "password": "admin123"
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "OK"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            },
            "404": {
              "description": "Not Found"
            }
          }
        }
      },
      "/register": {
        "post": {
          "tags": ["Auth"],
          "description": "User Register",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "username": "admin",
                    "email": "admin@email.com",
                    "password": "admin123",
                    "confirm_password": "admin123",
                    "isRestaurant": true
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Created"
            },
            "400": {
              "description": "Bad Request"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            },
            "409": {
              "description": "Conflict"
            }
          }
        }
      },
      "/userlist": {
        "get": {
          "tags": ["Users"],
          "description": "Get all users",
          "responses": {
            "200": {
              "description": "OK"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            }
          }
        }
      },
      "/user/{id}": {
        "get": {
          "tags": ["Users"],
          "description": "Get a user",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "User id",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int64"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "401": {
              "description": "Unauthorized"
            },
            "403": {
              "description": "Forbidden"
            },
            "404": {
              "description": "Not Found"
            }
          }
        }
      }
    }
  },
  "apis": ["./routes/*.js"]
}
